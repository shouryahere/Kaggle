# Environment Variables for Life Admin Concierge Agent (ADK)
# Copy this file to .env and fill in your values

# ============================================
# REQUIRED: Google API Key for ADK
# ============================================
# Get from: https://aistudio.google.com/apikey
GOOGLE_API_KEY=your_google_api_key_here

# Use Gemini API (not Vertex AI)
GOOGLE_GENAI_USE_VERTEXAI=FALSE

# ============================================
# OPTIONAL: For real Calendar/Gmail integration
# ============================================
# Google Cloud Credentials (for Calendar/Gmail API)
GOOGLE_CREDENTIALS_PATH=credentials.json

# Logging Level
LOG_LEVEL=INFO
